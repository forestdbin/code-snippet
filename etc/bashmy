PS1='\[\e]0;\h \A \w\a\]\[\e[31;1m\]\$\[\e[m\] '

HISTCONTROL=ignoreboth:erasedups

alias d='dirs -v'
alias p=pushd
alias q=popd

alias v=vim

alias ls='ls --color=auto'
alias l='ls -F'
alias la='ls -A'
alias ll='ls -l'
alias l1='ls -1'
alias l.='ls -d .*'

alias grep='grep --color=auto'
